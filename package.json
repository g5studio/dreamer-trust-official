{
  "name": "dreamergroup-official-website",
  "version": "0.0.16",
  "description": "",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "proxy-dev": "concurrently \"chii start -p 4321\" \"cross-env VITE_PROXY=true vite\"",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "serve-build": "cross-env VITE_SERVE_TEST=true vite build",
    "serve-build:watch": "concurrently \"chii start -p 4321\" \"cross-env VITE_PROXY=true VITE_SERVE_TEST=true VITE_BUILD_WATCH=true vite build --watch\" \"cross-env VITE_PROXY=true vite preview\"",
    "serve": "vite preview",
    "test": "vitest",
    "prepare": "husky install",
    "lint": "tsc --noEmit && eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w src/**/*.{ts,tsx} --config .prettierrc-cmd.js",
    "check-format": "npm run format -- --list-different",
    "format-and-lint": "npm run check-format && npm run lint",
    "check-all": "npm run format-and-lint && npm run test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "image-preprocess": "node scripts/image-preprocess.mjs",
    "image-preprocess-all": "node scripts/image-preprocess.mjs --run-all",
    "pwa-assets": "pwa-assets-generator && node scripts/generate-splash-screen.mjs"
  },
  "license": "MIT",
